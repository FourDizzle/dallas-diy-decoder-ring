{
  "name": "Dallas DIY Decoder Ring",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/events/*"
      ],
      "js": ["content.bundle.js"],
      "run_at" : "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
